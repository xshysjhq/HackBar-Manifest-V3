{
   "author": "Khoiasd",
   "background": {
      "service_worker": "background.js"
   },
   "description": "A HackBar for google chrome/firefox browser. Small tool for pentesting websercurity.",
   "devtools_page": "theme/hackbar.html",
   "homepage_url": "https://hackbar.herokuapp.com",
   "icons": {
      "48": "icons/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "HackBar",
   "permissions": [ "tabs", "activeTab", "webRequest", "declarativeNetRequest", "scripting", "storage" ],
   "host_permissions": [ "\u003Call_urls>" ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.1"
}
